    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "npm rebuild node-sass",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "proxy": "http://127.0.0.1:8080/login",
  "dependencies": {
    "axios": "~0.18.0",
    "bootstrap": "~4.3.1",
    "bootstrap-vue": "2.0.0-rc.11",
    "c3": "^0.6.7",
    "chart.js": "~2.7.2",
    "element-ui": "^2.8.2",
    "idle-vue": "~2.0.5",
    "ladda": "~2.0.1",
    "moment": "~2.22.1",
    "nouislider": "~12.0.0",
    "numeral": "~2.0.6",
    "perfect-scrollbar": "~1.4.0",
    "plyr": "~2.0.18",
    "simplemde": "~1.11.2",
    "spinkit": "~1.2.5",
    "sweet-modal-vue": "~2.0.0",
    "text-mask-addons": "~3.8.0",
    "v-img": "~0.2.0",
    "vue": "~2.6.6",
    "vue-awesome-swiper": "~3.1.3",
    "vue-axios": "^2.1.4",
    "vue-c3": "~1.2.9",
    "vue-chartjs": "~3.4.0",
    "vue-clipboard2": "~0.2.1",
    "vue-color": "~2.7.0",
    "vue-context-menu": "~2.0.6",
    "vue-cropper": "~0.4.0",
    "vue-dragula": "~1.3.1",
    "vue-echarts": "~3.1.3",
    "vue-flatpickr-component": "~8.0.0",
    "vue-form-wizard": "~0.8.4",
    "vue-gallery": "~1.4.0",
    "vue-i18n": "^8.11.2",
    "vue-input-tag": "~1.0.3",
    "vue-jstree": "~2.1.6",
    "vue-loading-overlay": "^3.2.0",
    "vue-masonry": "~0.11.1",
    "vue-meta": "~1.5.0",
    "vue-multiselect": "~2.1.0",
    "vue-notification": "~1.3.7",
    "vue-number-input-spinner": "~2.0.3",
    "vue-password-strength-meter": "~1.3.0",
    "vue-quill-editor": "~3.0.5",
    "vue-router": "~3.0.1",
    "vue-simple-calendar": "~4.1.0",
    "vue-simplemde": "~0.4.6",
